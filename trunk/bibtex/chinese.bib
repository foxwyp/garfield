@ARTICLE{zhanghua,
  author = {韩立岩 and 张华},
  title = {不完全竞争及不确定情况下R\&D投资额的决策研究},
  journal = {首都经济贸易大学学报},
  year = {2003},
  number = {02},
  abstract = {R&D是一个企业战略的重要组成部分。关于R&D投资的研究已成为国内外学者探讨的热点问题。然而,目前为止大多数的研究都基于R&D投资具有独占性和产品市场为完全竞争的市场两个假设,而没有考虑外界对R&D投资影响。本文研究了不完全竞争及不确定情况下,特别是双寡头垄断市场中当项目投资机会为非独占性时各寡头公司的R&D投资额决策问题。由于R&D投资额对研发速度存在影响,进而影响了企业未来收益,因此双寡头垄断的市场中各寡头之间R&D投资额的决策必然会相互影响。通过利用非合作博弈论方法建模,本文给出了这种情况下R&D投资额的纳什均衡解。},
  address = {北京航空航天大学经济管理学院,北京航空航天大学经济管理学院 北京 100083 ,北京 100083},
  isbn = {1008-2700},
  keywords = {博弈 and R&D and 投资决策}
}

